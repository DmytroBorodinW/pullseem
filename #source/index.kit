		<script
  src="https://code.jquery.com/jquery-3.5.1.slim.min.js"
  integrity="sha256-4+XzXVhsDmqanXGHaHvgh1gMQKX40OUvDEBTu8JcmNs="
  crossorigin="anonymous"></script>
  <script src='js/slick.min.js'></script> 
        <script src="https://cdn.amcharts.com/lib/4/core.js"></script>
        <script src="https://cdn.amcharts.com/lib/4/charts.js"></script> 
        <script>
            var slider = $('.charts__block').slick({
              infinite: false,
              slidesToShow: 3,
              slidesToScroll: 1,
              arrows: true,
              prevArrow: '<img src="imgs/left-arrow (1).svg" class="slide-arrow left-arrow" alt="">',
              nextArrow: '<img src="imgs/left-arrow (1).svg" class="slide-arrow right-arrow" alt="">',
               responsive: [
                    {
                      breakpoint: 993,
                      settings: {
                        slidesToShow: 1,
                        slidesToScroll: 1,
                        rtl: false,
                      }
                    },
                ]
            });

            $('.languages').change(function(e){
                var langVal = $(this).children("option:selected").val();
                if(langVal == 'true') {
                    slider.slick("slickSetOption", "rtl", true, false);
                    slider.slick('slickGoTo', 0);
                } else if(langVal == 'false'){
                    slider.slick("slickSetOption", "rtl", false, true);
                }
            })

        </script> 
